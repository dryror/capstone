				<Button id="editSite" title="Edit" onClick="editBtn"/>
			</RightNavButton>
			
			<TableView class="table" id="tbl">
				<HeaderView id="mainHeader">
					<View id="headerView">
						<Button id="addSite" title="Add Site Survey" onClick="addBtn"/>
					</View>
				</HeaderView>
				<FooterView id="mainFooter">
					<View id="footerView">
						<Button id="exportData" title="Export" onClick="exportBtn" />
					</View>
				</FooterView>
			</TableView>
		</Window>
	</NavigationWindow>
</Alloy>