<Alloy>
	<NavigationWindow id="navGroupWin">
		<Window class="container" title="Plot Observations">
			<LeftNavButton>
            	<Button id="editObservations" title="Edit" onClick="editBtn"/>
        	</LeftNavButton>
			<RightNavButton>
            	<Button id="addObservation" title="Add" onClick="addBtn"/>
        	</RightNavButton>
       
       		<TableView class="table">
				
				<FooterView id="tblFooter">
					<View id="footerView">
						<Label id="percentLbl" text="Total Percent Coverage: "/>
						<Label id="percent" text="2%"/>
						<Button id="done" title="Done" onClick="doneBtn" />
					</View>
				</FooterView>
	   		</TableView>
    	</Window>
    </NavigationWindow>
</Alloy>